    "iceberg": "<1.1.14",
    "iris": "<1.6.7",
    "immersive_portals": "<=3.2.5",
    "indium": "<1.0.24",
    "notenoughcrashes": "<4.4.6",
    "noxesium": "<1.0.2",
    "rayon": "<=1.7.2+1.20.1",
    "reeses-sodium-options": "<1.6.4",
    "sodium-extra": "<0.5.1",
    "sspb": "<3.2.0"
