    "preLaunch": [
      "me.jellysquid.mods.sodium.client.SodiumPreLaunch"
    ]
  },
  "custom": {
    "fabric-renderer-api-v1:contains_renderer": true,
    "modmenu": {
      "links": {
        "modmenu.donate": "https://caffeinemc.net/donate",
        "modmenu.discord": "https://caffeinemc.net/discord",
